<form class="d-inline" method="post" action="{{ path('subscription_delete', {'id': subs.getId()}) }}"
      onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ subs.getId()) }}">
    <button title="Supprimer" class="btn btn-sm btn-danger"><i
                class="material-icons">delete</i></button>
    </button>
</form>
